{
    "name": "marshmallow/components",
    "description": "Extendend Laravel Blade Components for Marshmallow",
    "keywords": [
        "marshmallow",
        "laravel",
        "components"
    ],
    "homepage": "https://github.com/marshmallow/components",
    "license": "MIT",
    "authors": [
        {
            "name": "Marshmallow",
            "email": "lars@marshmallow.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Components\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\Components\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Components\\ComponentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
